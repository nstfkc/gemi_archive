{
  "name": "gemi.js",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun server/dev.ts --hot",
    "test": "bun test",
    "prisma": "prisma",
    "db:generate": "prisma generate --schema=./db/schema.prisma",
    "db:studio": "prisma studio --schema=./db/schema.prisma",
    "db:push": "prisma db push --schema=./db/schema.prisma",
    "prebuild": "bun ./server/pre.ts",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build -c vite.config.server.js",
    "start": "NODE_ENV=production bun dist/server/prod.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hono/node-server": "^1.2.0",
    "@mantine/core": "^7.2.2",
    "@mantine/hooks": "^7.2.2",
    "@prisma/client": "latest",
    "@react-email/components": "^0.0.12",
    "@react-email/render": "^0.0.10",
    "@tabler/icons-react": "^2.42.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "history": "^5.3.0",
    "hono": "^3.9.2",
    "prisma": "latest",
    "react": "canary",
    "react-dom": "canary",
    "resend": "^2.0.0",
    "swr": "^2.2.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "latest",
    "@prisma/generator-helper": "^5.5.2",
    "@types/bun": "^1.0.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.11",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.32",
    "postcss-preset-mantine": "^1.11.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "typescript-parser": "^2.6.1",
    "vite": "^5.0.10"
  },
  "resolutions": {
    "react": "canary",
    "react-dom": "canary"
  }
}
